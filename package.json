{
	"name": "@techandtribal/raiden",
	"description": "Rapid NextJS deployment framework",
	"version": "1.224.0",
	"author": "Pete Savva <p.savva@protonmail.ch> (https://techandtribal.com)",
	"license": "MIT",
	"type": "module",
	"bin": {
		"raiden": "./bin/build.js"
	},
	"files": [
		"/bin/build.js",
		"/.eslintrc.json",
		"/README.md",
		"/rollup.config.js",
		"/filesToCopy/"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/baufometic/raiden#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/baufometic/raiden.git"
	},
	"bugs": {
		"url": "https://github.com/baufometic/raiden/issues"
	},
	"scripts": {
		"remove-build": "rm -f bin/build.js",
		"build-dev": "npm run remove-build && npm version minor && NODE_ENV=development rollup -c rollup.config.js",
		"build-prod": "npm run remove-build && npm version minor && NODE_ENV=production rollup -c rollup.config.js",
		"extract-all-tgz": "for file in *.tgz; do file_dir=${file%.tgz}; mkdir --parents $file_dir; tar -xvzf $file -C $file_dir; done"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.2",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-replace": "^4.0.0",
		"@types/node": "^17.0.23",
		"@types/rollup-plugin-peer-deps-external": "^2.2.1",
		"eslint": "^8.12.0",
		"identity-obj-proxy": "^3.0.0",
		"rollup": "^2.70.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-preserve-shebang": "^1.0.1"
	},
	"dependencies": {
		"colors": "^1.4.0"
	}
}
